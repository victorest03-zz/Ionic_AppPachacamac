<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.example411205" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>AppPachacamac</name>
  <description>
        Proyecto dise√±ado para el usuario de a pie, que desea realizar un viaje turistico a la ciudad de Pachacamac.
    </description>
  <author email="you@example.com" href="http://example.com.com/">
      Your Name Here
    </author>
  <content src="index.html"/>
  <access origin="*"/>
